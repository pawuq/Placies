{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost",
  "SigningPublicKey": {
    "SigningAlgorithm": "Ed25519", // RSA/Ed25519
    "PublicKeyPath": "./path/to/public-key.pem",
  },
  "ProxiedGateway": {
    "Address": "https://ipfs.io/"
  },
  "SigningGateway": {
    "CookieExpiration": "1.00:00:00",
  }
}
